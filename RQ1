#Agreement
#R Code
library(tidyverse)
library(psych)
df <- read_csv('ELI_Dec_9.csv')

# Select only the rater columns
rater_data <- df[, c("ChatGPT_5_Ex_Think_T1","Human_Gold_Stand")]  

#change variables to numeric
rater_data <- rater_data %>% mutate(across(everything(), as.numeric))

#Agreement
df %>% 
  mutate(agree = case_when(
    ChatGPT_5_Ex_Think_T1 == Human_Gold_Stand ~ 1,
    ChatGPT_5_Ex_Think_T1 != Human_Gold_Stand ~ 0,
    TRUE ~ NA_real_
  )) %>%
  summarise(agree_rate = mean(agree, na.rm = TRUE))

colSums(is.na(df[, c("ChatGPT_5_Ex_Think_T1", "Human_Gold_Stand")]))



